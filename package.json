{
  "name": "verdaccio-google-oauth-ui",
  "version": "0.1.2",
  "keywords": [
    "google",
    "login",
    "npm",
    "oauth",
    "package",
    "private",
    "registry",
    "verdaccio"
  ],
  "homepage": "https://github.com/alitheg/verdaccio-google-oauth-ui/blob/master/README.md",
  "bugs": "https://github.com/alitheg/verdaccio-google-oauth-ui/issues",
  "repository": "github:alitheg/verdaccio-google-oauth-ui",
  "license": "MIT",
  "author": "Alastair McFarlane",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "verdaccio-google-oauth-ui": "lib/cli/cli.js"
  },
  "scripts": {
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc --p tsconfig.server.json",
    "build:client": "tsc --p tsconfig.client.json && copy src\\client\\verdaccio-4.css lib\\public\\verdaccio-4.css",
    "lint": "eslint \"**/*.{js,ts}\"",
    "lint:fix": "eslint --fix \"**/*.{js,ts}\"",
    "prepack": "sh run clean && sh run build",
    "start": "verdaccio -c verdaccio.yaml",
    "test": "jest",
    "test:cli": "sh run oauthclitest",
    "test:coverage": "jest --collectCoverage && codecov",
    "test:manual": "yarn update && yarn start",
    "test:watch": "jest --watch",
    "test:whoami": "sh run whoami",
    "update": "yarn prepack && bash run link",
    "watch": "nodemon --watch src --ext ts,css --exec \"yarn update\""
  },
  "dependencies": {
    "express": "^4.17.0",
    "got": "^10.4.0",
    "jsonwebtoken": "^8.5.1",
    "memory-cache": "^0.2.0",
    "open": "^7.0.2"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.1",
    "@types/express": "^4.16.1",
    "@types/got": "^9.4.1",
    "@types/jest": "^25.1.1",
    "@types/lodash": "^4.14.123",
    "@types/node": "^13.7.0",
    "@verdaccio/types": "^9.3.0",
    "codecov": "^3.6.4",
    "core-js": "^3.6.4",
    "jest": "^25.1.0",
    "lodash": "4.17.15",
    "nodemon": "^2.0.2",
    "release-it": "^13.0.2",
    "typescript": "^3.7.5",
    "verdaccio": "^4.4.2"
  },
  "peerDependencies": {
    "lodash": "4",
    "verdaccio": "3 || 4"
  },
  "engines": {
    "node": ">=10"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "$schema": "http://json.schemastore.org/package"
}
