{
  "name": "verdaccio-google-oauth-ui",
  "version": "0.1.2",
  "keywords": [
    "google",
    "login",
    "npm",
    "oauth",
    "package",
    "private",
    "registry",
    "verdaccio"
  ],
  "homepage": "https://github.com/alitheg/verdaccio-google-oauth-ui/blob/master/README.md",
  "bugs": "https://github.com/alitheg/verdaccio-google-oauth-ui/issues",
  "repository": "github:alitheg/verdaccio-google-oauth-ui",
  "license": "MIT",
  "author": "Alastair McFarlane",
  "files": [
    "dist"
  ],
  "main": "dist/server.js",
  "bin": {
    "verdaccio-google-oauth-ui": "dist/cli.js"
  },
  "scripts": {
    "build": "npm run build:types",
    "build:types": "tsc",
    "lint": "eslint \"**/*.{js,ts}\"",
    "lint:fix": "eslint --fix \"**/*.{js,ts}\"",
    "prepack": "sh run clean && sh run build",
    "start": "verdaccio -c verdaccio.yaml",
    "test": "jest",
    "test:cli": "sh run oauthclitest",
    "test:coverage": "jest --collectCoverage && codecov",
    "test:manual": "yarn update && yarn start",
    "test:watch": "jest --watch",
    "test:whoami": "sh run whoami",
    "update": "yarn prepack && bash run link",
    "watch": "nodemon --watch src --ext ts,css --exec \"yarn update\""
  },
  "dependencies": {
    "express": "^4.17.0",
    "got": "^10.4.0",
    "memory-cache": "^0.2.0",
    "open": "^7.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/got": "^9.4.1",
    "@types/jest": "^25.1.1",
    "@types/lodash": "^4.14.123",
    "@types/node": "^13.7.0",
    "@verdaccio/types": "^9.3.0",
    "codecov": "^3.6.4",
    "core-js": "^3.6.4",
    "jest": "^25.1.0",
    "lodash": "4.17.15",
    "nodemon": "^2.0.2",
    "release-it": "^13.0.2",
    "rollup": "^2.0.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.2.0",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5",
    "verdaccio": "^4.4.2",
    "verdaccio-htpasswd": "^9.3.0"
  },
  "peerDependencies": {
    "lodash": "4",
    "verdaccio": "3 || 4"
  },
  "engines": {
    "node": ">=10"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "$schema": "http://json.schemastore.org/package"
}
